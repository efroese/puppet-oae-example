service.pid="org.sakaiproject.nakamura.lite.storage.jdbc.JDBCStorageClientPool"
service.bundleLocation="slinginstall:org.sakaiproject.nakamura.core-<%= version_nakcore %>.jar"
jdbc-driver="<%= sparsedriver %>"
jdbc-url="<%= sparseurl %>
username="<%= sparseuser %>"
password="<%= sparsepass %>"
when-exhausted-action="grow"
num-tests-per-eviction-run=I"1000"
time-between-eviction-run=L"60000"
min-evictable-idle-time-millis=L"10000"
test-while-idle=B"false"
long-string-size=I"0"
max-wait=L"10"
max-idle=I"5"
max-active=I"200"
test-on-borrow=B"true"
test-on-return=B"true"
